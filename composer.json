{
    "name": "chadicus/csv",
    "description": "Collection of classes to work with csv files",
    "keywords": ["csv", "delimited", "file", "io"],
    "config": {
        "sort-packages": true
    },
    "license": "MIT",
    "require": {
        "php": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "satooshi/php-coveralls": "~0.6.1",
        "squizlabs/php_codesniffer": "^3.2"
    },
    "autoload": {
        "psr-4": { "Chadicus\\Csv\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "ChadicusTest\\Csv\\": "tests/" }
    }
}
