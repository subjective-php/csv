{
    "name": "chadicus/csv",
    "description": "Collection of classes to work with csv files",
    "keywords": ["csv", "delimited", "file", "io"],
    "authors": [
        {
            "name": "Chad Gray",
            "email": "chadwickgray@gmail.com",
            "role": "Developer"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "license": "MIT",
    "require": {
